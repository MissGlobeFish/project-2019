/* pages/rights/rights.wxss */
.backView {
    width: 100%;
    height: 100%;
    background-color: #fff
}

.headerBack {
    background: #F4F4FB;
    width: 100%;
    height: 221rpx;
    padding-left: 40rpx;
    padding-top: 50rpx;
    position: relative;
}
.headerImage {
    position: absolute;
    right: 0;
    top: 0;
    width: 94rpx;
    height: 123rpx;
}
.curreLevelLab {
    font-family: PingFangSC-Medium;
    font-size: 16px;
    color: #353535;
}
.levelLab {
    font-family: PingFangSC-Medium;
    font-size: 16px;
    color: #FF6508;
}
.levelDesLab {
    margin-top: 10rpx;
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #808080;
    display: block;
}

.listBack {
    position: relative;
    margin-left: 40rpx;
    margin-right: 40rpx;
    min-height: 500rpx;
    margin-top: -52rpx;
    padding-bottom: 200rpx;
}

.cell {
    background: #FFFFFF;
    box-shadow: inset 0 -1px 0 0 #E8E8E8;
    min-height: 80rpx;
    padding-top: 24rpx;
    padding-bottom: 24rpx;
    display: flex;
    align-items: center
}
.cellNameLab {
    font-family: PingFangSC-Regular;
    font-size: 12px;
    color: #808080;
    display: inline-block;
    text-align: center;
    width: 50%;
}

.contentText {
    color:rgba(53,53,53,1);
    padding-left: 25rpx;
    padding-right: 25rpx;
}
.contenValueText {
    color:rgba(199,116,46,1);
}